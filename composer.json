{
    "name": "itszun/laravel-bootstrap-table",
    "description": "Package for rendering bootstrap table using laravel",
    "type": "library",
    "require": {
        "laravel/laravel": ">=8.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Itszun\\LaravelBootstrapTable\\": "src/"
        },
        "files" : [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Itszun",
            "email": "malikal.j4@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Itszun\\LaravelBootstrapTable\\Providers\\BootstrapTableServiceProvider"
            ],
            "aliases": {
                "BootstrapTable": "Itszun\\LaravelBootstrapTable\\Table"
            }
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
